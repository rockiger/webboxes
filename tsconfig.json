{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ESNEXT",
    "paths": {
      "*": ["node_modules/*"]
    },
    "jsx": "react"
  },
  "include": ["src/**/*", "AddBoxDialog"]
}
